{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Global": {
        "TimeOut": "10:00:00"
      },
      "Hosting": {
        "Debug": true
      },
      "RequestHandler": {
        "AddTrailingSlash": false
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Timestamp"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "MatthewDotCare.Contracts.DocumentTypes",
        "AcceptUnsafeModelsDirectory": true,
        "ModelsDirectory": "~/../MatthewDotCare.Contracts/DocumentTypes"
      }
    }
  },
  "MaxImageDimension": 1920,
  "PictureElement": {
    "DefaultQuality": 100,
    "Breakpoints": {
      "preview": 0,
      "desktopLarge": 1200,
      "desktop": 960,
      "tablet": 640,
      "mobile": 0
    }
  }
}
