@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<HomePage>

<div class="m-home-wrapper">
    <div class="uk-grid uk-width uk-flex-middle uk-flex-between">
        <div class="uk-width-1-3@l uk-text-center">
            <picture>
                <source srcset="https://unsplash.it/id/1062/620/800" media="(min-width:1200px)" width="620" height="800">
                <source srcset="https://unsplash.it/id/1062/250/250" media="(min-width:640px)" width="250" height="250">
                <img alt="" src="https://unsplash.it/id/1062/200/200" width="200" height="200" decoding="async" loading="lazy">
            </picture>
        </div>
        <div class="uk-width-1-3@l">
            <div class="uk-container uk-container-xsmall">
                <div class="uk-text-center uk-text-left@l">
                    <h1 class="uk-text-uppercase">@Model.Heading <span class="uk-display-block uk-text-primary">@Model.SubHeading</span></h1>
                </div>
                <div class="uk-text-lead">
                    <p>@Model.Content</p>
                </div>
            </div>
        </div>
       <partial name="Partials/Navigation/MainNavigation" />
    </div>
</div>